  "scripts": {
    "prestart": "OUTPUT_FILE=public/pluginConfig.json yarn run generateConfig",
    "start": "node ./scripts/start.js",
    "build": "node ./scripts/build.js",
    "clean": "rm -rf ./build && rm -rf ./cli",
    "cli:build": "webpack --config config/webpack.config.cli.js",
    "postbuild": "yarn run generateConfig",
    "lint": "eslint src/",
    "test": "node ./scripts/test.js",
    "generateConfig": "NODE_ENV=production babel-node --presets @babel/env,@babel/preset-react bin/generateConfigFile.js",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/preset-env": "^7.16.11",
